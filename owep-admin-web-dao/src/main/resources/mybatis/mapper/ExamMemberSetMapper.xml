<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kclm.owep.mapper.ExamMemberSetMapper">

    <insert id="save" parameterType="com.kclm.owep.entity.ExamMemberSet">
        insert into t_exam_memberset(exam_id,user_id,class_id)
        value (#{exam.id},
                #{user.id},
                #{clazz.id})
    </insert>

    <update id="update" parameterType="com.kclm.owep.entity.ExamMemberSet">
        update t_exam_memberset set  user_id=#{user.id},class_id=#{clazz.id}  where id=#{id};
    </update>

    <update id="deleteById"></update>

    <update id="deleteSelect"></update>

    <select id="selectById"></select>

    <select id="selectAll" resultType="com.kclm.owep.entity.ExamMemberSet">
        select * from t_exam_memberset
    </select>

</mapper>