        <mapper resource="mybatis/mapper/GroupMapper.xml"/>
        <mapper resource="mybatis/mapper/RoleMapper.xml"/>
        <mapper resource="mybatis/mapper/MenuMapper.xml"/>
        <!--End-->

        <mapper resource="mybatis/mapper/ReleaseMapper.xml"/>
        <mapper resource="mybatis/mapper/ReleaseLogMapper.xml"/>

