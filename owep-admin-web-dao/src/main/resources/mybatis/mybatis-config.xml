<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- 全局设置 -->
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <!-- 在持久层中单元测试时，临时使用，通过控制器启动时，此行需要去掉 -->
        <setting name="logImpl" value="STDOUT_LOGGING"/>
    </settings>

    <!--<typeHandlers>
        <typeHandler handler="org.apache.ibatis.type.EnumOrdinalTypeHandler"
                                javaType="com.kclm.owep.entity.UserStatus"/>
    </typeHandlers>-->

    <!--<typeAliases>
        <package name="com.kclm.owep.entity"/>
    </typeAliases>-->

    <!-- 在其它的映射文件没有修改好的情况下，需要对自己的mapper进行测试时使用，以后会删除掉 -->
    <mappers>
        <mapper resource="mybatis/mapper/AdviceMapper.xml"/>
        <mapper resource="mybatis/mapper/ReleaseMapper.xml"/>
        <mapper resource="mybatis/mapper/ReleaseLogMapper.xml"/>
        <mapper resource="mybatis/mapper/QuestionMapper.xml"/>
        <mapper resource="mybatis/mapper/QuestionReplyMapper.xml"/>
        <mapper resource="mybatis/mapper/ClazzMapper.xml"/>
        <mapper resource="mybatis/mapper/ProfessionMapper.xml"/>
        <!--yejx 开始-->
        <mapper resource="mybatis/mapper/AssignRecordMapper.xml"/>
        <mapper resource="mybatis/mapper/PlanManagerMapper.xml"/>
        <mapper resource="mybatis/mapper/ChannelMapper.xml"/>
        <mapper resource="mybatis/mapper/ChannelTypeMapper.xml"/>
        <mapper resource="mybatis/mapper/ChapterMapper.xml"/>
        <mapper resource="mybatis/mapper/ClientMapper.xml"/>
        <mapper resource="mybatis/mapper/ClientNoteMapper.xml"/>
        <mapper resource="mybatis/mapper/CourseCategoryMapper.xml"/>
        <mapper resource="mybatis/mapper/CourseCommentMapper.xml"/>
        <mapper resource="mybatis/mapper/CourseMapper.xml"/>
        <mapper resource="mybatis/mapper/CourseReportMapper.xml"/>
        <mapper resource="mybatis/mapper/PlanManagerCourseMapper.xml"/>
        <mapper resource="mybatis/mapper/SectionMapper.xml"/>
        <mapper resource="mybatis/mapper/SectionVideoMapper.xml"/>
        <!--yejx结束-->
        <mapper resource="mybatis/mapper/NewsMapper.xml"/>
        <mapper resource="mybatis/mapper/NoticeMapper.xml"/>
    </mappers>

</configuration>
